package ayuda;

import java.awt.EventQueue;

import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import javax.swing.JLabel;
import java.awt.Color;
import java.awt.Font;
import javax.swing.JButton;

public class AcercaDe extends JFrame {

	private JPanel contentPane;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					AcercaDe frame = new AcercaDe();
					frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the frame.
	 */
	public AcercaDe() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 450, 261);
		contentPane = new JPanel();
		contentPane.setBorder(new EmptyBorder(5, 5, 5, 5));

		setContentPane(contentPane);
		contentPane.setLayout(null);
		
		JLabel lblTienda = new JLabel("Tienda de ceramicas");
		lblTienda.setFont(new Font("Tahoma", Font.BOLD, 14));
		lblTienda.setForeground(Color.BLACK);
		lblTienda.setBounds(141, 11, 157, 49);
		contentPane.add(lblTienda);
		
		JLabel lblAutor = new JLabel("Hecho por Maichol Landeo ,Emilio Cabrera ,Randy , Keiber");
		lblAutor.setFont(new Font("Tahoma", Font.PLAIN, 13));
		lblAutor.setBounds(59, 71, 336, 25);
		contentPane.add(lblAutor);
		
		JLabel lblAño = new JLabel("2023");
		lblAño.setFont(new Font("Tahoma", Font.PLAIN, 13));
		lblAño.setBounds(191, 107, 46, 25);
		contentPane.add(lblAño);
		
		JLabel lblLimaPeru = new JLabel("Lima ,Peru");
		lblLimaPeru.setFont(new Font("Tahoma", Font.PLAIN, 13));
		lblLimaPeru.setBounds(179, 143, 98, 25);
		contentPane.add(lblLimaPeru);
		
		JButton btnCerrar = new JButton("Cerrar");
		btnCerrar.setBounds(165, 179, 89, 23);
		contentPane.add(btnCerrar);
